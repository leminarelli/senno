<style>
    :root {
        --color-blue: #04486E;
    }

    /* Radios container */
    div.colors-container {
        display: inline-block;
        /* width: calc(1.5em * 8);
        height: calc(2em * 2);
        padding: .5em; */
    }

    /* Radios container row */
    div.colors-container-row {
        /* margin-bottom: .3em;
        padding-top: .1em; */
    }

    .medal-avatar, .circle-avatar {
        /* border: 1px solid rgba(0,0,0,1); */
        border-radius: 50%;
        width: 5em;
        height: 5em;
        display: flex;
        justify-content: center;
        align-content: center;
        align-items: center;
        color: white;
    }

    .medal-avatar {
        width: 100px !important;
        height: 100px !important;
        font-size: 48px !important;
    }

    .circle-avatar {
        width: 85px;
        height: 85px;
        font-size: 43px;
    }

    .medal-avatar-lg, .circle-avatar-lg {
        width: 150px !important;
        height: 150px !important;
        font-size: 72px !important;
    }

    .circle-avatar-sm {
        font-size: 14px !important;
    }

    .medal-avatar-text, .circle-avatar-text {
        /* font-size: 1.75em; */
        margin: 0;
    }

    /*
    ** Especifico para Grupos More
    */
    .grupo-more, .coautores-more {
        color: black;
    }

    .cover .grupo-more, .cover .coautores-more {
        color: white !important;
        border-color: white !important;
    }

    /*
    ** PLATAFORMA EDIT
    */
    .plataforma-edit .progressbar li.active {
        color: var(--plataforma-background-color);
    }
    .plataforma-edit #progressbar li.active:before, #progressbar li.active:after {
        background: var(--plataforma-background-color);
    }
    .plataforma-edit #msform .action-button {
        background: var(--plataforma-background-color);
    }
    /* Steps Qty */
    .plataforma-edit #progressbar li, .plataforma-edit .progressbar li {
        width: calc(100% / 6);
    }
    .plataforma-edit #progressbar li .wizard-step-inner-circle, #progressbar li .wizard-step-outer-circle {
        stroke: gray !important;
    }

    .plataforma-edit #progressbar li.active .wizard-step-inner-circle, #progressbar li.active .wizard-step-outer-circle {
        stroke: var(--plataforma-background-color) !important;
    }
    .plataforma-edit #progressbar li.active .wizard-step-inner-circle {
        fill: var(--plataforma-background-color) !important;
    }

    .page-loader {
        width: 160px;
    }

    .page-loader .loader-logo {
        width: 100%;
        text-align: center;
        margin-bottom: 20px;
    }

    .page-loader .loader-logo img {
        width: 75%;
    }

    .page-loader .loader-bar {
        background-color: #eee;
        height: 3px;
        border-radius: 1em;
        width: 100%;
        overflow: hidden;
        position: relative;
        z-index: 1;
    }

    .page-loader .loader-bar .loader-indicator {
        background-color: var(--color-blue);
        width: 50px;
        height: 100%;
        border-radius: 10px;
        animation: loader 1s ease-in-out infinite;
    }

    @keyframes loader {
        0%, 100% {
            transform: translateX(-10px);
        }
        50% {
            transform: translateX(130px);
        }
    }

    .loading {
        position: relative;
        background-color: transparent;
        overflow: hidden;
    }

    .loading::after {
        display: block;
        content: '';
        position: absolute;
        width: 100%;
        height: 100%;
        transform: translateX(-100%);
        background: linear-gradient(90deg, transparent, rgba(255, 255, 255, .5), transparent);
        animation: loading 1.5s infinite;
        top: 0;
    }

    .card-title.loading {
        width: 10rem;
        height: 1rem;
        overflow: hidden;
    }

    @keyframes loading {
        100% {
            transform: translateX(100%);
        }
    }

</style>
