{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es6": true
    },
    
    
    "parserOptions": {
        "sourceType": "module"
    },

    "plugins": [
        "import",
        "react"
    ],

    "globals": {
        "io": true,
        "lodash": true,
        "log": true,
        "classnames": true,
        "moment": true,
        "$": true,
        "echarts": true
    },

    "rules": {
        "no-unused-vars": 0,
        "react/react-in-jsx-scope": 0,
        "react/prop-types": 1,
        "no-console": 0
    }
    
}